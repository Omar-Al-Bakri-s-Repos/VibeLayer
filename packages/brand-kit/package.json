{
  "name": "@vibelayer/brand-kit",
  "version": "0.1.0",
  "description": "VibeLayer Brand Kit - Brand consistency scoring and analysis algorithms",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    }
  },
  "scripts": {
    "build": "tsup src/index.ts --format cjs,esm --dts",
    "dev": "tsup src/index.ts --format cjs,esm --dts --watch",
    "lint": "eslint \"src/**/*.{ts,tsx}\"",
    "type-check": "tsc --noEmit",
    "test": "jest",
    "clean": "rimraf dist"
  },
  "dependencies": {
    "@vibelayer/shared": "workspace:*"
  },
  "devDependencies": {
    "@types/node": "^22.10.1",
    "eslint": "^8.57.0",
    "jest": "^29.7.0",
    "rimraf": "^5.0.5",
    "tsup": "^8.0.0",
    "typescript": "^5.6.3"
  },
  "publishConfig": {
    "access": "restricted"
  }
}