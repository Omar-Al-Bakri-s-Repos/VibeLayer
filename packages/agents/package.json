{
  "name": "@vibelayer/agents",
  "version": "0.1.0",
  "description": "VibeLayer AI Agents - Python-based LangGraph + PydanticAI integration types",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    }
  },
  "scripts": {
    "build": "tsup src/index.ts --format cjs,esm --dts",
    "dev": "tsup src/index.ts --format cjs,esm --dts --watch",
    "lint": "eslint \"src/**/*.{ts,tsx}\"",
    "type-check": "tsc --noEmit",
    "clean": "rimraf dist"
  },
  "dependencies": {
    "@vibelayer/shared": "workspace:*",
    "@langchain/langgraph": "^0.2.0",
    "@langchain/core": "^0.3.0",
    "@google-ai/generativelanguage": "^2.6.0",
    "@google-cloud/speech": "^6.7.0",
    "langchain": "^0.3.0"
  },
  "devDependencies": {
    "@types/node": "^22.10.1",
    "eslint": "^8.57.0",
    "rimraf": "^5.0.5",
    "tsup": "^8.0.0",
    "typescript": "^5.6.3"
  },
  "publishConfig": {
    "access": "restricted"
  }
}
